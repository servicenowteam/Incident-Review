        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	<g2:evaluate var="jvar_review_stage">
		var review_stage = '$[current.u_review_stage]';
		review_stage;
	</g2:evaluate>
	<g2:evaluate var="jvar_html_wins">
		var html_wins = "$[current.u_review_html_wins]";
		html_wins;
	</g2:evaluate>
	<g2:evaluate var="jvar_html_improvements">
		var html_improvements = "$[current.u_review_html_improvements]";
		html_improvements;
	</g2:evaluate>
	<j2:if test="$[jvar_review_stage == 'technician review' || jvar_review_stage == 'closed']">
		<j2:if test="$[!empty(jvar_html_wins)]">
		<div class="container" style="margin-bottom:10px;">
			
			<div class="row">
				<div class="col-xs-12" style="padding-left:0px;">
					<h3>Wins</h3>
				</div>
			</div>
			<div class="row" style="border: 1px solid #bdc0c4;border-radius:2px;padding:10px;">
				<div class="col-xs-12" style="padding:5px;">
					<g2:no_escape>$[jvar_html_wins]</g2:no_escape>
				</div>
			</div>
		</div>
		</j2:if>
		<j2:if test="$[!empty(jvar_html_improvements)]">
		<div class="container">
			
			<div class="row">
				<div class="col-xs-12" style="padding-left:0px;">
					<h3>Improvements</h3>
				</div>
			</div>
			<div class="row" style="border: 1px solid #bdc0c4;border-radius:2px;padding:10px;">
				<div class="col-xs-12" style="padding:5px;">
					<g2:no_escape>$[jvar_html_improvements]</g2:no_escape>
				</div>
			</div>
		</div>
		</j2:if>
	</j2:if>
</j:jelly>]]></xml>
    </sys_ui_macro>
</record_update>
